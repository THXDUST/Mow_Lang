STACK.ALLOC 4

LABEL start
STACK.PUSH int READ int "Insert a number: "
STACK.PRINT
JUMP EQ STACK.TOP, 0 L1
JUMP LT STACK.TOP, 0 inv

LABEL LOOP
STACK.PUSH int SUB 2, STACK.POP
STACK.PRINT
JUMP EQ STACK.TOP, 0 L1
JUMP GT STACK.TOP, 0 LOOP
PRINTL "odd"
EXIT

LABEL L1
PRINTL "even"
EXIT

LABEL inv
PRINTL "\nPlease insert a positive number."
STACK.POP
JUMP start
