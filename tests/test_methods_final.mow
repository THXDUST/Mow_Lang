PRINTL("===== STRINGS TESTS =====");
STR msg = "hello world";
PRINTL("Original: " + msg);
PRINTL("Upper: " + msg.UPPER());
PRINTL("Lower: " + msg.LOWER());
PRINTL("Length: " + msg.LENGTH());
PRINTL("Reversed: " + msg.REVERSE());
PRINTL("Starts with 'hello': " + msg.STARTSWITH("hello"));
PRINTL("Ends with 'world': " + msg.ENDSWITH("world"));
PRINTL("Contains 'world': " + msg.CONTAINS("world"));
PRINTL("Replace 'world' with 'MowLang': " + msg.REPLACE("world", "MowLang"));

// ===== TESTES COM LISTAS =====
PRINTL("");
PRINTL("===== TESTES COM LISTAS =====");
LIST myList = [1, 2, 3, 4, 5];
PRINTL("Original list LENGTH: " + myList.LENGTH());
myList.APPEND(6);
PRINTL("After APPEND(6), LENGTH: " + myList.LENGTH());
PRINTL("Contains 3: " + myList.CONTAINS(3));
PRINTL("Count of 3: " + myList.COUNT(3));
PRINTL("Index of 3: " + myList.INDEX(3));

